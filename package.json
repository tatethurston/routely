{
  "name": "PACKAGE_NAME",
  "version": "0.0.1",
  "description": "PACKAGE_DESCRIPTION",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/PACKAGE_NAME"
  },
  "main": "dist/index.js",
  "files": [
    "dist/index.d.ts"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && yarn webpack",
    "build:watch": "yarn build --watch",
    "clean": "rm -rf dist/*",
    "lint": "yarn typecheck && prettier --check . && prettier-package-json --list-different package.json && eslint .",
    "lint:fix": "prettier --write . && prettier-package-json --write package.json && eslint --fix .",
    "lint:fix:md": "prettier --write '*.md'",
    "lint:fix:package": "prettier-package-json --write package.json",
    "lint:fix:ts": "eslint --fix './src/**/*.ts'",
    "test": "yarn build && jest --testTimeout 5000 --rootDir dist",
    "typecheck": "yarn tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch",
    "version": "yarn run build && git add -A package.json",
    "postversion": "git push && git push --tags"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.1",
    "prettier-package-json": "^2.1.3",
    "typescript": "^4.1.3",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "keywords": [
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  }
}
